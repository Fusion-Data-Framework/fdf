<?xml version='1.0'?>
<container name='chers' mdstree='activespec' mdspath='\chers_best'>
    <method name='plot'></method>
    <node name='valid' mdsnode='valid'></node>
    <axis name='radius' units='cm' mdsnode='radius'></axis>
    <axis name='time' units='s' mdsnode='time'></axis>
    <signal name='ti' units='keV' error='dti' axes='radius, time' mdsnode='ti'></signal>
    <signal name='vt' units='km/s' error='dvt' axes='radius,time' mdsnode='vt'></signal>
    <signal name='ft' units='Hz' error='dft' axes='radius,time' mdsnode='ft'></signal>
    <signal name='nc' units='1e13/cm^3' error='dnc' axes='radius,time' mdsnode='nc'></signal>
    <container name='derived'>
        <node name='valid' mdsnode='valid'></node>
        <signal name='zeff' units='' error='dzeff' axes='radius,time' mdsnode='zeff'></signal>
        <signal name='nd' units='1e13/cm^3' error='dnd' axes='radius,time' mdsnode='nd'></signal>
        <signal name='pi' units='kPa' error='dpi' axes='radius,time' mdsnode='pi'></signal>
    </container>
    <container name='spline'>
        <node name='valid' mdsnode='svalid'></node>
        <axis name='radius' units='cm' mdsnode='rs'></axis>
        <signal name='tis' units='keV' axes='radius, time' mdsnode='tis'></signal>
        <signal name='vts' units='km/s' axes='radius,time' mdsnode='vts'></signal>
        <signal name='zeffs' units='' axes='radius,time' mdsnode='zeffs'></signal>
        <signal name='ncs' units='1e13/cm^3' axes='radius,time' mdsnode='ncs'></signal>
    </container>
</container>
